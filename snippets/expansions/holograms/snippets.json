{
    "Drupi Holograms | HologramManager": {
		"prefix": "holograms.HologramManager",
		"body": "holograms.HologramManager();",
		"description": "See more at https://github.com/sainttx/Holograms\n\nReturns HologramManager"
    },
    "Drupi Holograms | Create": {
		"prefix": "holograms.Create",
		"body": "holograms.Create(\"${1:ID}\", ${2:Location});",
		"description": "Create new holograms"
    },
    "Drupi Holograms | Get": {
		"prefix": "holograms.get",
		"body": "holograms.get(\"${1:ID}\");",
		"description": "Get holograms \n\n Returns String"
    },
    "Drupi Holograms | Delete": {
		"prefix": "holograms.delete",
		"body": "holograms.delete(${1:Hologram});",
		"description": "Delete holograms"
    },
    "Drupi Holograms | Move": {
		"prefix": "holograms.move",
		"body": "holograms.move(${1:Hologram}, ${2:Location});",
		"description": "Move holograms in different location"
    },
    "Drupi Holograms | Hide": {
		"prefix": "holograms.hide",
		"body": "holograms.hide(${1:Hologram});",
		"description": "Hide holograms"
    },
    "Drupi Holograms | Show": {
		"prefix": "holograms.show",
		"body": "holograms.show(${1:Hologram});",
		"description": "Show holograms"
